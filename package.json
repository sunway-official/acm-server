{
  "name": "cem-server",
  "version": "1.0.0",
  "description": "Backend project for CEM",
  "main": "src/index.js",
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "prestart": "npm i -g nodemon",
    "start": "nodemon build/index.js",
    "build:only": "babel --minified --no-comments -d ./build ./src",
    "build-all": "run-s clean:build build:*",
    "dev": "cross-env NODE_PATH=/ nodemon -e js --exec babel-node src/index.js",
    "migrate:latest:dev": "knex migrate:latest --knexfile src/knexfile.js",
    "migrate:rollback:dev": "knex migrate:rollback --knexfile src/knexfile.js",
    "seed:dev": "knex seed:run --knexfile src/knexfile.js",
    "reset:db:dev": "run-s migrate:rollback:dev migrate:latest:dev seed:dev",
    "migrate:latest:staged": "knex migrate:latest --knexfile src/knexfile.js --env staged",
    "migrate:rollback:staged": "knex migrate:rollback --knexfile src/knexfile.js --env staged",
    "seed:staged": "knex seed:run --knexfile src/knexfile.js --env staged",
    "reset:db:staged": "run-s migrate:rollback:staged migrate:latest:staged seed:staged",
    "doc:gen": "graphdoc -f",
    "lint": "esw *.js src config --color",
    "lint-staged": "lint-staged",
    "pretty": "prettier --single-quote --trailing-comma all --write '{src,tests}/**/*.js'",
    "precommit": "run-s lint-staged lint",
    "clean:build": "rimraf ./build",
    "clean:docs": "rimraf ./docs",
    "clean-all": "run-s clean:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunway-team/cem-server.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunway-team/cem-server/issues"
  },
  "homepage": "https://github.com/sunway-team/cem-server#readme",
  "lint-staged": {
    "{src,tests}/**/*.js": [
      "prettier --single-quote --trailing-comma all --print-width 80 --tab-width 2 --write",
      "git add"
    ]
  },
  "graphdoc": {
    "endpoint": "http://localhost:8080/graphql",
    "output": "./docs"
  },
  "dependencies": {
    "apollo-server-express": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.0",
    "faker": "^4.1.0",
    "graphql": "^0.10.5",
    "graphql-subscriptions": "^0.5.4",
    "graphql-tools": "^1.2.3",
    "graphql-tools-types": "^1.1.12",
    "helmet": "^3.8.2",
    "jsonwebtoken": "^8.0.1",
    "knex": "^0.13.0",
    "merge-graphql-schemas": "^1.1.4",
    "nodemailer": "^4.1.3",
    "objection": "^0.8.9",
    "objection-unique": "^0.2.1",
    "optics-agent": "^1.1.7",
    "pg": "^7.3.0",
    "subscriptions-transport-ws": "^0.8.3",
    "yup": "^0.22.0"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@istanbuljs/nyc-config-babel": "^1.2.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-watch": "^3.1.2",
    "graphql-request": "^1.4.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "prettier": "^1.7.2",
    "prettier-eslint": "^8.2.0",
    "rimraf": "^2.6.2"
  }
}
