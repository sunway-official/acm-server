{
  "name": "cem-server",
  "version": "1.0.0",
  "description": "Backend project for CEM",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_PATH=/ nodemon -e js,graphql --exec babel-node src/index.js",
    "migrate:latest": "knex migrate:latest --knexfile src/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile src/knexfile.js",
    "doc": "graphdoc -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunway-team/cem-server.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunway-team/cem-server/issues"
  },
  "homepage": "https://github.com/sunway-team/cem-server#readme",
  "graphdoc": {
    "endpoint": "http://localhost:5000/graphql",
    "output": "./doc/schema"
  },
  "dependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "apollo-server-express": "^1.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "casual": "^1.5.17",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "graphql": "^0.10.5",
    "graphql-subscriptions": "^0.4.4",
    "graphql-tools": "^1.2.1",
    "graphql-tools-types": "^1.1.11",
    "helmet": "^3.8.1",
    "jsonwebtoken": "^7.4.3",
    "knex": "^0.13.0",
    "merge-graphql-schemas": "^1.1.2",
    "objection": "^0.8.8",
    "objection-unique": "^0.2.0",
    "optics-agent": "^1.1.6",
    "pg": "^7.2.0",
    "subscriptions-transport-ws": "^0.8.2",
    "yup": "^0.21.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-babel": "^1.2.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-watch": "^3.1.2",
    "graphql-request": "^1.3.4",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.1.0",
    "prettier": "^1.5.3",
    "prettier-eslint": "^6.4.2"
  }
}
